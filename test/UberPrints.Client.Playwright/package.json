{
  "name": "uberprints-client-playwright",
  "version": "1.0.0",
  "description": "End-to-end tests for UberPrints client",
  "scripts": {
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:manual": "playwright test --config=playwright.config.manual.ts",
    "test:manual:ui": "playwright test --config=playwright.config.manual.ts --ui",
    "test:manual:headed": "playwright test --config=playwright.config.manual.ts --headed",
    "report": "playwright show-report",
    "codegen": "playwright codegen http://localhost:5173"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.47.0",
    "@types/node": "^22.5.0",
    "testcontainers": "^10.13.2",
    "wait-on": "^8.0.1"
  }
}
